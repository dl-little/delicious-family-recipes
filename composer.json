{
	"name": "doug/delicious-family-recipes",
	"config": {
		"vendor-dir": "vendor",
		"optimize-autoloader": true
	},
	"autoload": {
		"psr-4": {
			"DFR\\": "includes/"
		}
	},
	"scripts": {
		"build:admin": "sass ./scss/adminStyles.scss ./admin/css/adminStyles.css",
		"watch:admin": "sass --watch ./scss/adminStyles.scss ./admin/css/adminStyles.css",
		"watch:main": "sass --watch ./scss/main.scss ./public/css/main.css",
		"build:main": "sass ./scss/main.scss ./public/css/main.css"
	},
	"require": {}
}
