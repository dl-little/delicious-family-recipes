{
	"name": "doug/delicious-family-recipes",
	"config": {
		"vendor-dir": "lib",
		"optimize-autoloader": true
	},
	"autoload": {
		"psr-4": {
			"DFR\\": "src/"
		}
	},
	"scripts": {
		"build-admin-styles": "sass ./assets/adminStyles.scss ./assets/dist/adminStyles.css",
		"watch-admin-styles": "sass --watch ./assets/adminStyles.scss ./assets/dist/adminStyles.css",
		"watch-main-styles": "sass --watch ./assets/main.scss ./assets/dist/main.css",
		"build-main-styles": "sass ./assets/main.scss ./assets/dist/main.css"
	},
	"require": {}
}
